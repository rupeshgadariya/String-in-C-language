// Write a program to convert a given string into uppercase
#include<stdio.h>
#include<conio.h>

 void main()
{
        char ch[20];
    printf("Enter string \n");
    fgets(ch,20,stdin);
    for (int i = 0; ch[i]; i++)
        if(ch[i]>='a' && ch[i]<='z')
         ch[i] =ch[i]-32;
    printf(ch);
        
    getch();
}
