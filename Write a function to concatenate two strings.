// Write a function to concatenate two strings.
#include<stdio.h>

  void con(char str1[], char str2[])
  {    int i;
      for (i = 0; str1[i]; i++);
      str1[i]=' ';
        i++;
      for( int j=0; str2[j]; j++)
        {
          str1[i] = str2[j];
           i++;
        }
     
      str1[i]='\0';
  }



    int main()
{
       char str[50]="hello";
       char ch[50]= "world";

      con(str,ch);
      puts(str);
        
    return 0;
}
