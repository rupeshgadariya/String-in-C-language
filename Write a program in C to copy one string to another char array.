// Write a program in C to copy one string to another char array.

#include<stdio.h>
#include<conio.h>

    void main()
{       int i;
        char ch[20], cpy[20];
    printf("Enter a string ");
    fgets(ch,20,stdin);
printf("%s\n",ch);
    for ( i = 0; i < 20; i++)
    {
        if(ch[i]!='\n')
         cpy[i] = ch[i];
        else break;
    }
       cpy[i]='\0';

printf("%s",cpy);
        
    getch();
}
