// Write a function to make first character of each word of the string capital. 
#include<stdio.h>

  void cap(char str[])
  { 
    int i=0;

    for (int i = 0; str[i]; i++)
     
        if(i==0 || str[i-1]==' ' || str[i-1]=='\n' || str[i-1]=='\t')
          if( str[i]>='a' && str[i]<='z')
            str[i]=str[i]-32;
    }


    int main()
{
    char str[20];
    fgets(str,20,stdin);
     cap(str);
    printf("%s",str);       
        
    return 0;
}
