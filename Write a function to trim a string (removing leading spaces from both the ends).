// Write a function to trim a string (removing leading spaces from both the ends). 
#include<stdio.h>
#include<conio.h>

    void trim(char str[])
{
    int n;
       for(n=0; str[n]; n++)
        if(str[n]!=' ')
         break;

    for(int j=0; j<n; j++)
      for (int i = 0; str[i]; i++)
        str[i] = str[i+1];
      
    for(n=0; str[n]; n++);

    while (n)
    {   
        if (str[n-1]==' ' || str[n-1]=='\n' )
          str[n-1]=str[n];
        else
          break;
        
       n--;
    }
}
    
     
  
    


int main()
{
    char str[20]=" Hello world ";
    fgets(str,20,stdin);
    trim(str);
 printf("%skumar",str);



}
