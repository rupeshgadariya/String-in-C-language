// Write a function to store strings, taken from user, to the given 2d char array. 
// #include<stdio.h>
#include <stdio.h>

void input(char str[][20], int x) {
    for (int i = 0; i < x; i++) {
        printf("Enter string %d: ", i + 1);
        fgets(str[i], 20, stdin);
    }
}

int main() {
    char str[2][20];

    input(str, 2);

  
    for (int i = 0; i < 2; i++)
        printf("%s", str[i]);

    return 0;
}
